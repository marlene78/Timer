<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
          <!-- Custom fonts for this template-->
        <link href="{{ asset('fontawesome-free/css/all.min.css')  }}" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

        <!-- Custom styles for this template-->
        <link href="{{ asset('css/sb-admin-2.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/loader.css') }}" rel="stylesheet">
        <link href="{{ asset('css/style.css') }}" rel="stylesheet">
        <link href="{{ asset('css/bootstrap-datetimepicker.min.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
       <script src="https://cdn.tiny.cloud/1/z8prk9090hyncsy4saa9bdhguqb1ownzqvv9m4sn3mb3idum/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>

        {% block stylesheets %}{% endblock %}
    </head>
<body id="page-top">


  <!-- Page Wrapper -->
  <div id="wrapper">
          
    <!-- navbar -->
    {% include 'includes/_nav.html.twig' %}
    <!-- End of navbar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <!-- Topbar -->
        {% include 'includes/_topbar.html.twig' %}
        <!-- End of Topbar -->
                  
        <div class="container-fluid">
        
          {% for label, messages in app.flashes %}

              {% if label == "success" %}

                  {% for message in messages %}
                      <div class="alert alert-{{ label }}">
                          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                          <i class="fa fa-check"></i>
                          {{ message }}
                    
                      </div>
                  {% endfor %}

              {% else %}


                  {% for message in messages %}
                      <div class="alert alert-{{ label }}">
                          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                            <i class="fa fa-times"></i>
                          {{ message }}
                      </div>
                  {% endfor %}

      
              {% endif %}
            {% endfor %}


          {% block body %}{% endblock %}
      
        </div>
             

      </div>


      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span>Copyright &copy; ms@Réclam 2020</span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->
      
    </div>



  </div>
    

  <!-- End of Wrapper -->



  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Êtes-vous sûre de vouloir vous déconnecter</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">Non</button>
          <a class="btn btn-primary" href="{{ path('logout')  }}">Oui</a>
        </div>
      </div>
    </div>
  </div>





          <!-- Bootstrap core JavaScript-->
        <script src="{{ asset('jquery/jquery.min.js')  }}"></script>
        <script src="{{ asset('bootstrap/js/bootstrap.bundle.min.js')  }}"></script>

        <!-- Core plugin JavaScript-->
        <script src="{{ asset('jquery-easing/jquery.easing.min.js')  }}"></script>

        <!-- Custom scripts for all pages-->
        <script src="{{ asset('js/sb-admin-2.min.js') }}"></script>

        <!-- Page level plugins -->
        <script src="{{ asset('chart.js/Chart.min.js')  }}"></script>

        <!-- Page level plugins -->
        <script src="{{ asset('datatables/jquery.dataTables.min.js') }}"></script>
        <script src="{{ asset('datatables/dataTables.bootstrap4.min.js') }}"></script>

        <!-- Page level custom scripts -->
        <script src="{{ asset('js/demo/datatables-demo.js')  }}"></script>

        <!-- Password -->
        <script src="{{ asset('js/password.js')  }}"></script>

        <!-- Form -->
        <script src="{{ asset('js/form.js')  }}"></script>
        <!-- Modal -->
        <script src="{{ asset('js/modal.js')  }}"></script>

{% block javascripts %}{% endblock %}

    </body>
</html>
